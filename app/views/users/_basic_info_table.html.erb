<table>
  <thead>
  <tr>
    <td>Name</td>
    <td>Website</td>
    <td>Short Url</td>
    <td># of Friends</td>
  </tr>
  </thead>
  <tbody>
  <% users.each do |user| %>
      <tr>
        <td><%= link_to user.full_name,user_path(user) %></td>
        <td><%= user.website_full_url %></td>
        <td><%= user.google_short_url %></td>
        <td><%= user.all_friends.count %></td>
      </tr>
  <% end %>
  </tbody>
</table>